  "configurations": [
    {
      "name": "Python Debugger: Module",
      "type": "python",
      "request": "launch",
      "module": "algo_trade"
    },
    {
      // Launch the current file but under the module path using the -m flag
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
    },
